	<queryString>
		<![CDATA[SELECT
      CONCAT(cc.codigo,' - ',c.descricao) AS cargo_descricao,
      conc.titulo AS concurso_titulo,
      l.nome AS localidade_nome,
      (SELECT COUNT(*) FROM inscricao i WHERE i.cargo_concurso_id = cc.id) AS qtd_cargo
FROM
    cargo_concurso cc,
    cargo c,
    localidade l,
    concurso conc
WHERE
     cc.cargo_id = c.id AND
     cc.localidade_id = l.id AND
     cc.concurso_id = conc.id AND
     conc.id = $P{id_concurso}
ORDER BY
      l.nome,
      cc.codigo]]>
	</queryString>
