<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-ejb-jar PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 EJB 3.1//EN" "http://glassfish.org/dtds/glassfish-ejb-jar_3_1-1.dtd">
<glassfish-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>FuncionarioService</ejb-name>
      <resource-ref>
        <res-ref-name>java:comp/DefaultJMSConnectionFactory</res-ref-name>
        <jndi-name>java:comp/DefaultJMSConnectionFactory</jndi-name>
      </resource-ref>
      <message-destination-ref>
        <message-destination-ref-name>jms/emailQueue</message-destination-ref-name>
        <jndi-name>jms/emailQueue</jndi-name>
      </message-destination-ref>
    </ejb>
    <ejb>
      <ejb-name>EnviaEmailService</ejb-name>
      <resource-ref>
        <res-ref-name>jms/emailQueueFactory</res-ref-name>
        <jndi-name>jms/emailQueueFactory</jndi-name>
      </resource-ref>
      <resource-ref>
        <res-ref-name>java:comp/DefaultJMSConnectionFactory</res-ref-name>
        <jndi-name>java:comp/DefaultJMSConnectionFactory</jndi-name>
      </resource-ref>
      <message-destination-ref>
        <message-destination-ref-name>jms/emailQueue</message-destination-ref-name>
        <jndi-name>jms/emailQueue</jndi-name>
      </message-destination-ref>
    </ejb>
  </enterprise-beans>
</glassfish-ejb-jar>
